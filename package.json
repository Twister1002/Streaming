{
  "name": "streaming",
  "version": "0.0.1",
  "description": "Streaming overlay and integrations with other services",
  "main": "main.js",
  "author": "Twister1002",
  "scripts": {
    "start": "electron .",
    "gulp": "gulp watch",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32  --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Bandito\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Twister1002/Streaming.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Twister1002/Streaming/issues"
  },
  "dependencies": {
    "axios": "0.17.1"
  },
  "devDependencies": {
    "electron": "1.7.10",
    "electron-packager": "10.1.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0",
    "browser-sync": "2.23.1"
  },
  "homepage": "https://github.com/Twister1002/Streaming#readme"
}
